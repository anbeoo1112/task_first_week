# ===========================================
# GOOGLE GEMINI API (AI Studio)
# ===========================================
# Lấy tại: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY="AIzaSy..."

# ===========================================
# GOOGLE DOCUMENT AI (OCR)
# ===========================================
# Project ID: Google Cloud Console > Dashboard > Project Info
DOCUMENTAI_PROJECT_ID="your-project-id"

# Vùng tạo Processor (us, eu, asia-southeast1, ...)
DOCUMENTAI_LOCATION="us"

# Processor ID: Document AI > Processors > Copy ID
DOCUMENTAI_PROCESSOR_ID="xxxxxxxxxxxxxxxx"

# Đường dẫn đến file credentials JSON (Service Account)
# Tải tại: IAM & Admin > Service Accounts > Keys > Add Key > JSON
DOCUMENTAI_GOOGLE_CREDENTIALS="./credentials.json"
